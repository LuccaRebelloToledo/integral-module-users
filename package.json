{
  "name": "integral-module-for-managing-all-user-related-operations",
  "version": "1.0.0",
  "description": "This module is an integral part of an application, responsible for managing all user-related operations.",
  "main": "index.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "node ./dist/shared/infra/http/index.js",
    "start:dev": "nodemon --exec ts-node -r tsconfig-paths/register ./src/shared/infra/http/index.ts",
    "build": "tsc && tsc-alias && npm run sentry:sourcemaps",
    "eslint": "eslint ./src --max-warnings 0 --fix",
    "prettier": "prettier --write ./src",
    "lint-staged": "lint-staged",
    "test": "jest --passWithNoTests",
    "test:watch": "npm run test --watch",
    "test:staged": "npm run test --findRelatedTests",
    "test:push": "npm run test --coverage",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/shared/infra/http/data-source.ts migration:generate ./src/shared/infra/typeorm/migrations/default",
    "migration:revert": "typeorm-ts-node-commonjs -d ./src/shared/infra/http/data-source.ts migration:revert",
    "migration:run": "typeorm-ts-node-commonjs -d ./src/shared/infra/http/data-source.ts migration:run",
    "prepare": "husky",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org lucca-toledo --project node-express ./ && sentry-cli sourcemaps upload --org lucca-toledo --project node-express ./"
  },
  "repository": {
    "type": "git",
    "url": "git+/LuccaRebelloToledo/expressjs-user-module.git"
  },
  "author": "LuccaRebelloToledo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LuccaRebelloToledo/expressjs-user-module/issues"
  },
  "homepage": "https://github.com/LuccaRebelloToledo/expressjs-user-module#readme",
  "dependencies": {
    "@sentry/cli": "^2.28.0",
    "@sentry/node": "^7.100.1",
    "@sentry/profiling-node": "^7.100.1",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.3.0",
    "celebrate": "^15.0.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.1",
    "escape-html": "^1.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.3.2",
    "joi": "^17.12.1",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^3.3.7",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "swagger-ui-express": "^5.0.0",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/escape-html": "^1.0.4",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.15",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^1.7.1",
    "git-commit-msg-linter": "^5.0.6",
    "husky": "^9.0.8",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.1",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
