{
  "name": "expressjs-user-module",
  "version": "1.0.0",
  "description": "ExpressJs User API",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/shared/infra/http/index.js",
    "build": "tsc",
    "dev": "nodemon --exec ts-node ./src/shared/infra/http/index.ts",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/shared/infra/http/data-source.ts migration:generate ./src/shared/infra/typeorm/migrations/default",
    "migration:revert": "typeorm-ts-node-commonjs -d ./src/shared/infra/http/data-source.ts migration:revert",
    "migration:run": "typeorm-ts-node-commonjs -d ./src/shared/infra/http/data-source.ts migration:run"
  },
  "repository": {
    "type": "git",
    "url": "git+/LuccaRebelloToledo/expressjs-user-module.git"
  },
  "author": "LuccaRebelloToledo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LuccaRebelloToledo/expressjs-user-module/issues"
  },
  "homepage": "https://github.com/LuccaRebelloToledo/expressjs-user-module#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "celebrate": "^15.0.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^3.2.0",
    "dotenv": "^16.3.1",
    "escape-html": "^1.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^3.3.7",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.19"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/escape-html": "^1.0.4",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
