{
  "private": true,
  "name": "integral-module-for-managing-all-user-related-operations",
  "version": "1.0.0",
  "description": "This module is an integral part of an application, responsible for managing all user-related operations.",
  "main": "index.js",
  "author": "LuccaRebelloToledo",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node ./dist/shared/infra/http/index.js",
    "start:dev": "nodemon --exec ts-node -r tsconfig-paths/register ./src/shared/infra/http/index.ts",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "lint": "npx @biomejs/biome lint --write ./src",
    "format": "npx @biomejs/biome format --write ./src",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --passWithNoTests --watch",
    "test:push": "jest --passWithNoTests --collectCoverage",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate ./src/shared/infra/typeorm/migrations/%npm_config_name% -d ./src/shared/infra/typeorm/data-sources/data-source.ts",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./src/shared/infra/typeorm/data-sources/data-source.ts",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/shared/infra/typeorm/data-sources/data-source.ts",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@sentry/node": "^8.43.0",
    "@sentry/profiling-node": "^8.43.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.3.0",
    "celebrate": "^15.0.3",
    "class-transformer": "^0.5.1",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "escape-html": "^1.0.3",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^8.0.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.12.1",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^3.3.8",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.1",
    "swagger-ui-express": "^5.0.0",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/escape-html": "^1.0.4",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^22.10.1",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.7",
    "git-commit-msg-linter": "^5.0.6",
    "husky": "^9.0.8",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.11",
    "nodemon": "^3.1.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  }
}
